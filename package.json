{
	"name": "Play YouTube Video In Background",
	"version": "1.0.0",
	"author": "Labinator",
	"type": "module",
	"description": "Prevents YouTube & Vimeo from stopping Firefox from playing videos in the background.",
	"repository": {
		"type": "git",
		"url": "https://github.com/LabinatorSolutions/play-youtube-video-in-background.git"
	},
	"homepage": "https://github.com/LabinatorSolutions/play-youtube-video-in-background",
	"bugs": {
		"url": "https://github.com/LabinatorSolutions/play-youtube-video-in-background/issues"
	},
	"scripts": {
		"lint": "eslint . --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint . --fix",
